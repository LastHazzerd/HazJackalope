  {
    "op": "addMerge",
    "dependsOn": [ { "modid": "harepet" } ],
    "path": "/client/behaviors/-",
    "file": "hazjackalope:entities/animal/mammal/hare/jackalope-adult.json",
    "value": [
      { "code": "multiply", "enabledByType": { "*-female": true, "*": false } }
    ]
  }, //client side adult multiply
  {
    "op": "addEach",
    "dependsOn": [ { "modid": "harepet" } ],
